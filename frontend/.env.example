# Vite client env vars (exposed to the browser if prefixed with VITE_)

# API base URL for the BFF. Recommended: keep it relative and use Vite proxy.
VITE_API_BASE_URL=/api/v1

# Dev/preview proxy target for requests under /api/*
# (Used by Vite config only; not referenced in client code.)
VITE_API_PROXY_TARGET=http://127.0.0.1:8002

# Optional: seed admin token for local development
VITE_ADMIN_TOKEN=
